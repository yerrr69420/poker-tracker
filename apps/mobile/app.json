{
  "expo": {
    "name": "Poker Tracker",
    "slug": "poker-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mascot.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/mascot.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0e1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pokertracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mascot.png",
        "backgroundColor": "#0a0e1a"
      },
      "package": "com.pokertracker.app"
    },
    "plugins": [
      "expo-haptics"
    ]
  }
}
